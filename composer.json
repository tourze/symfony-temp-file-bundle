{
  "name": "tourze/symfony-temp-file-bundle",
  "description": "临时文件管理",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/console": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/event-dispatcher": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "symfony/service-contracts": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\TempFileBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\TempFileBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0",
    "shipmonk/composer-dependency-analyser": "^1.8"
  }
}