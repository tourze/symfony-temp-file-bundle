{
  "name": "tourze/symfony-temp-file-bundle",
  "description": "Symfony临时文件集中管理与自动清理bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/service-contracts": "^3.6"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/symfony-dependency-service-loader": "1.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\TempFileBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\TempFileBundle\\Tests\\": "tests"
    }
  }
}